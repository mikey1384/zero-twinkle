{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "process": true
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-dupe-keys": "error",
    "no-undef": "error",
    "no-duplicate-imports": "error"
  }
}
